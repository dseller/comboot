<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>

  <appSettings>
    <add key="SerialPort" value="COM4"/>
    <add key="WriteBufferSize" value="2097152"/>
    <add key="BaudRate" value="9600"/>

    <!-- File 0: stage 2 loader -->
    <add key="File0" value="stage2.bin"/>
    
    <!-- File 1: kernel -->
    <add key="File1" value="kernel.sys"/>
  </appSettings>
</configuration>